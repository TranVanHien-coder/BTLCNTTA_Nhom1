DEF contacden Transform {
	rotation 0 1 0 -1.57
	translation	43.8 3 38
	children [
		Shape {
			geometry Box {
				size 0.05 0.05 0.015
			}
			appearance Appearance {
				material Material {
					diffuseColor .39 .46 .42
					specularColor .53 .53 .53
					ambientIntensity .103
					shininess .93
				}
			}
		}

	DEF	nut Transform {
			translation	0 0 0.01
			rotation 1 0 0 1.57
			children [
				Shape {
					geometry Cylinder {
						height 0.005
						radius 0.01
						bottom TRUE
						top	TRUE
						side TRUE
					}
					appearance Appearance {
						material Material {
							diffuseColor .88 .15 .01
							specularColor .19 .03 .03
							ambientIntensity .0833
							shininess .08
						}
					}
				}
			]
		}

		DEF	touch1 TouchSensor {
			enabled	TRUE
		}

		DEF	time3 TimeSensor {
			enabled	FALSE
			loop TRUE
		}

		DEF ma Script {
			eventIn	SFBool turnOn
			eventOut SFBool	on
			eventOut SFBool	off
			field SFBool turn FALSE	
			url	"javascript:
			function turnOn(val){
				if(val){
					turn = !turn;
					if(turn)
					{	
				  on = true; off = false; 	  
				 
					}
  					else
       				{  
					on = false; off = true;	 
					}
    			}
			}
			"
		}
	]
	ROUTE touch1.isActive TO ma.turnOn
	ROUTE ma.on	TO time3.enabled
	ROUTE time3.isActive TO	plBDen.on
	ROUTE time3.isActive TO	plBDen2.on
}


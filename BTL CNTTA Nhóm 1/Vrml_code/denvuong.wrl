#VRML V2.0 utf8

Transform {
	children [
		Shape {
			geometry Box {
				size 0.1 0.03 0.1
			}
			appearance Appearance {
				material Material {
					diffuseColor 1 .85 0
					specularColor .87 .25 .25
					ambientIntensity .157
					shininess 1
				}
			}
		}

		DEF plDen PointLight { 
		on FALSE 
		intensity 1
		ambientIntensity 0 
		color 1 1 1		
		location 0 -0.5 0 
		attenuation -1 0 0 
		radius 30
		}
	    
	]
}


#VRML V2.0 utf8

DEF phanquay 
Transform {
	children [Transform {
	translation	0.1 0 0.15
	children [
		Shape {
			appearance Appearance {
				material Material {
					diffuseColor .88 .15 .01
					specularColor .19 .03 .03
					ambientIntensity .0833
					shininess .08
				}
			}
			geometry Box {size 0.2 0.015 0.3}
		}
	]
}]
}

Transform {
	translation	0.1 0 0.15
	children [
		Shape {
			appearance Appearance {
				material Material {
					diffuseColor .88 .15 .01
					specularColor .19 .03 .03
					ambientIntensity .0833
					shininess .08
				}
			}
			geometry Box {size 0.2 0.015 0.3}
		}
	]
}

DEF	click TouchSensor {}
DEF	time TimeSensor	{
	cycleInterval 5
	enabled	FALSE
	loop TRUE-
}
DEF	control Script {
	eventIn	SFBool turn
	eventOut SFBool	on
	eventOut SFBool	off
	field SFBool x TRUE
	url	"javascript:
	function turn(val){
		if(val){
			if(x){
				on = true;
				off = false
			}else{
				on = false;
				off = true;
			}
			x=!x;
		}
	}
	"
}
DEF	ori OrientationInterpolator	{
	key	[0 0.5 1]
	keyValue [0 0 1 0, 0 0 1 01.57, 0 0 1 3.14]
}
ROUTE click.isActive TO control.turn
ROUTE control.on TO	time.enabled
ROUTE time.fraction_changed TO	ori.set_fraction
ROUTE ori.value_changed	TO phanquay.rotation
